{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Interphase",
    "type": "object",
    "properties": {
        "MolarVolume": {
            "type": "number",
            "minimum": 1e-06,
            "maximum": 0.03,
            "description": "Volume per amount of substance.",
            "unit": "m\u00b3\u00b7mol\u207b\u00b9"
        },
        "IonicConductivity": {
            "type": [
                "string",
                "object",
                "number"
            ],
            "minimum": 1.0e-10,
            "maximum": 1.0e3,
            "description": "Measure of the ionic conduction of ions in the electrolyte under given concentration and temperature conditions",
            "context_type": [
                "IonicConductivity",
                "Expression"
            ],
            "context_type_iri": "https://w3id.org/emmo/domain/electrochemistry#electrochemistry_25dabdc2_68bf_4a38_8cbe_11be017358bc",
            "unit": "S·m⁻¹",
            "unit_name": "emmo:SiemensPerMetre",
            "unit_iri": "https://w3id.org/emmo#SiemensPerMetre",
            "documentation": "https://battmoteam.github.io/BattMo.jl/dev/manuals/user_guide/simulation_dependent_input"
        },
        "ElectronicDiffusionCoefficient": {
            "type": "number",
